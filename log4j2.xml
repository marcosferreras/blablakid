<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="error" name="Blablakid" packages="">
	<Appenders>
		<File name="MyFile" fileName="logs/blablakid.log">
     		   	<PatternLayout>
        			<Pattern>%d %p %c{1.} [%t] %m%n</Pattern>
    			</PatternLayout>
   		</File>
	</Appenders>
	<Loggers>
		<Root level="all">
			<AppenderRef ref="MyFile"/>
		</Root>
	</Loggers>
</Configuration>
